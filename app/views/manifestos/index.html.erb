<h1>Listing manifestos</h1>

<table>
  <tr>
    <th>Politician</th>
    <th>Title</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @manifestos.each do |manifesto| %>
  <tr>
    <td><%= manifesto.politician_id %></td>
    <td><%= manifesto.title %></td>
    <td><%= manifesto.description %></td>
    <td><%= link_to 'Show', manifesto %></td>
    <td><%= link_to 'Edit', edit_manifesto_path(manifesto) %></td>
    <td><%= link_to 'Destroy', manifesto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Manifesto', new_manifesto_path %>
