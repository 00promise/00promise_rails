<h1>Listing reply_evaluations</h1>

<table>
  <tr>
    <th>Reply</th>
    <th>User</th>
    <th>Eval type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reply_evaluations.each do |reply_evaluation| %>
  <tr>
    <td><%= reply_evaluation.reply_id %></td>
    <td><%= reply_evaluation.user_id %></td>
    <td><%= reply_evaluation.eval_type %></td>
    <td><%= link_to 'Show', reply_evaluation %></td>
    <td><%= link_to 'Edit', edit_reply_evaluation_path(reply_evaluation) %></td>
    <td><%= link_to 'Destroy', reply_evaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

